        <class_name>Processor</class_name>

        <class_source><![CDATA[int DeviceID;
String DevicePropertyName;
String DevicePropertyConfidence;
String DevicePropertyComponentClass;
String DevicePropertyComponentInstance;
String DevicePropertyDataType;
String DevicePropertyValue;

public boolean processRow( StepMetaInterface smi, StepDataInterface sdi ) throws KettleException {

  // First, get a row from the default input hop
  Object[] r = getRow();

  // If the row object is null, we are done processing.
  if (r == null) {
    setOutputDone();
    return false;
  }

  // Get the data from the current row
  String value_xml_data_type_description = get( Fields.In, "xml_data_type_description" ).getString(r);
  String value_xml_element_id = get( Fields.In, "xml_element_id" ).getString(r);
  String value_xml_parent_element_id = get( Fields.In, "xml_parent_element_id" ).getString(r);
  String value_xml_element_level = get( Fields.In, "xml_element_level" ).getString(r);
  String value_xml_path = get( Fields.In, "xml_path" ).getString(r);
  String value_xml_parent_path = get( Fields.In, "xml_parent_path" ).getString(r);
  String value_xml_data_name = get( Fields.In, "xml_data_name" ).getString(r);
  String value_xml_data_value = get( Fields.In, "xml_data_value" ).getString(r);

  // Clear stored data for new device
  if ( value_xml_data_type_description.equals( "START_ELEMENT" ) && value_xml_path.equals( "/xml/device" ) ) {
    DeviceID = 0;
    DevicePropertyName = null;
    DevicePropertyConfidence = null;
    DevicePropertyComponentClass = null;
    DevicePropertyComponentInstance = null;
    DevicePropertyDataType = null;
    DevicePropertyValue = null;
  }

  // Set DeviceID for new device
  if ( value_xml_data_type_description.equals( "ATTRIBUTE" ) && value_xml_path.equals( "/xml/device" ) && value_xml_data_name.equals( "id" ) ) {    
    DeviceID = Integer.parseInt( value_xml_data_value );
  }

  // Clear stored data for new device property
  if ( value_xml_data_type_description.equals( "START_ELEMENT" ) && value_xml_path.equals( "/xml/device/properties/property" ) ) {
    DevicePropertyName = null;
    DevicePropertyConfidence = null;
    DevicePropertyComponentClass = null;
    DevicePropertyComponentInstance = null;
    DevicePropertyDataType = null;
    DevicePropertyValue = null;
  }

  // Set property attribute for device
  if ( value_xml_data_type_description.equals( "ATTRIBUTE" ) && value_xml_path.equals( "/xml/device/properties/property" ) ) {
    if ( value_xml_data_name.equals( "name" ) ) {
      DevicePropertyName = value_xml_data_value;
    }
    if ( value_xml_data_name.equals( "confidence" ) ) {
      DevicePropertyConfidence = value_xml_data_value;
    }
    if ( value_xml_data_name.equals( "component_class" ) ) {
      DevicePropertyComponentClass = value_xml_data_value;
    }
    if ( value_xml_data_name.equals( "component_instance" ) ) {
      DevicePropertyComponentInstance = value_xml_data_value;
    }
    if ( value_xml_data_name.equals( "data_type" ) ) {
      DevicePropertyDataType = value_xml_data_value;
    }
  }

  // Set property value for device, create output row
  if ( value_xml_data_type_description.equals( "CHARACTERS" ) && value_xml_path.equals( "/xml/device/properties/property" ) ) {
    DevicePropertyValue = value_xml_data_value;

    Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
    get( Fields.Out, "DeviceID" ).setValue( outputRow, DeviceID );
    get( Fields.Out, "ComponentClass" ).setValue( outputRow, DevicePropertyComponentClass );
    get( Fields.Out, "Name" ).setValue( outputRow, DevicePropertyName );
    get( Fields.Out, "DataType" ).setValue( outputRow, DevicePropertyDataType );
    get( Fields.Out, "Value" ).setValue( outputRow, DevicePropertyValue );

    putRow( data.outputRowMeta, outputRow );
  }

  return true;
}]]></class_source>
        </definition>
