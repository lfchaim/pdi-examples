      <formula_string>"&lt;http://example.org/" + CustomerID + "> &lt;http://example.org/CustomerID> \"" + CustomerID + "\" &lt;http://example.org/CustomerType/" + CustomerType + "> . \n" + "&lt;http://example.org/" + CustomerID + "> &lt;http://example.org/FirstName> \"" + CustomerFirstName + "\" &lt;http://example.org/CustomerType/" + CustomerType + "> . \n" + "&lt;http://example.org/" + CustomerID + "> &lt;http://example.org/Surname> \"" + CustomerSurname + "\" &lt;http://example.org/CustomerType/" + CustomerType + "> . \n" + "&lt;http://example.org/" + CustomerID + "> &lt;http://example.org/CustomerID> \"" + CustomerID + "\" &lt;http://example.org/CustomerType/" + CustomerType + "> . \n" + "&lt;http://example.org/" + CustomerID + "> &lt;http://example.org/HasOrder> &lt;http://example.org/Order/" + OrderID + "> &lt;http://example.org/CustomerType/" + CustomerType + "> . \n" + "&lt;http://example.org/Order/" + OrderID + "> &lt;http://example.org/OrderID> \"" + OrderID + "\" &lt;http://example.org/Orders> . \n" + "&lt;http://example.org/Order/" + OrderID + "> &lt;http://example.org/Amount> \"" + OrderAmount + "\"^^&lt;http://www.w3.org/2001/XMLSchema#double> &lt;http://example.org/Orders> . \n"</formula_string>
