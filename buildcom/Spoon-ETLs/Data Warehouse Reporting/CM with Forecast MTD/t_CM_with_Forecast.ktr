    <sql>select &#xd;&#xa;	&#x5b;Major Category&#x5d;,&#xd;&#xa;	SubCategory,&#xd;&#xa;	Team,&#xd;&#xa;	sum&#x28;&#x5b;PY Gross Margin Dollars&#x5d;&#x29; as &#x27;PY Gross Margin Dollars&#x27;,&#xd;&#xa;	sum&#x28;&#x5b;PY Total Revenue with Freight In&#x5d;&#x29; as &#x27;PY Total Revenue with Freight In&#x27;,&#xd;&#xa;	sum&#x28;&#x5b;CY Gross Margin Dollars&#x5d;&#x29; as &#x27;CY Gross Margin Dollars&#x27;,&#xd;&#xa;	sum&#x28;&#x5b;CY Total Revenue with Freight In&#x5d;&#x29; as &#x27;CY Total Revenue with Freight In&#x27;,&#xd;&#xa;	sum&#x28;&#x5b;Forecast Margin&#x5d;&#x29; as &#x27;Forecast Margin&#x27;,&#xd;&#xa;	sum&#x28;&#x5b;Forecast Revenue&#x5d;&#x29; as &#x27;Forecast Revenue&#x27;,&#xd;&#xa;	sum&#x28;Prev_Month_TGM&#x29; as Prev_Month_TGM,&#xd;&#xa;	sum&#x28;Prev_Month_MS&#x29; as Prev_Month_MS,&#xd;&#xa;	sum&#x28;Curr_Month_TGM&#x29; as Curr_Month_TGM,&#xd;&#xa;	sum&#x28;Curr_Month_MS&#x29; as Curr_Month_MS,&#xd;&#xa;	sum&#x28;&#x5b;Backorder Margin&#x5d;&#x29; as &#x27;Backorder Margin&#x27;,&#xd;&#xa;	sum&#x28;&#x5b;Backorder Revenue&#x5d;&#x29; as &#x27;Backorder Revenue&#x27;,&#xd;&#xa;	sum&#x28;Modified_NCM&#x29; as Modified_NCM,&#xd;&#xa;	sum&#x28;Net_Revenue&#x29; as Net_Revenue,&#xd;&#xa;	sum&#x28;RPR&#x29; as RPR,&#xd;&#xa;	sum&#x28;Gross_Revenue&#x29; as Gross_Revenue,&#xd;&#xa;	sum&#x28;Modified_NCM_Forecast&#x29; as Modified_NCM_Forecast,&#xd;&#xa;	sum&#x28;Rev_Forecast&#x29; as Rev_Forecast,&#xd;&#xa;	sum&#x28;Return_Rev_Forecast&#x29; as Return_Rev_Forecast&#xd;&#xa;from&#xd;&#xa;&#x28;&#xd;&#xa;	SELECT        &#xd;&#xa;		isnull&#x28;CFO_Data.topCategory, Forecast.topCategory&#x29; &#x27;Major Category&#x27;,&#xd;&#xa;		isnull&#x28;CFO_Data.subCategory, Forecast.subCategory&#x29; &#x27;SubCategory&#x27;, &#xd;&#xa;		CFO_Data.Team, &#xd;&#xa;		isnull&#x28;CFO_Data.P1_GM_DLLS, 0&#x29; &#x27;PY Gross Margin Dollars&#x27;, &#xd;&#xa;		isnull&#x28;CFO_Data.P1_TRwFI, 0&#x29; &#x27;PY Total Revenue with Freight In&#x27;, &#xd;&#xa;		isnull&#x28;CFO_Data.P2_GM_DLLS, 0&#x29; &#x27;CY Gross Margin Dollars&#x27;, &#xd;&#xa;		isnull&#x28;CFO_Data.P2_TRwFI, 0&#x29; &#x27;CY Total Revenue with Freight In&#x27;, &#xd;&#xa;		isnull&#x28;Forecast.Margin_calc, 0&#x29; &#x27;Forecast Margin&#x27;, &#xd;&#xa;		isnull&#x28;Forecast.Revenue_calc, 0&#x29; &#x27;Forecast Revenue&#x27;, &#xd;&#xa;		0 as Prev_Month_TGM,&#xd;&#xa;		0 as Prev_Month_MS,&#xd;&#xa;		0 as Curr_Month_TGM,&#xd;&#xa;		0 as Curr_Month_MS,&#xd;&#xa;		Backorders.EST_MARGIN &#x27;Backorder Margin&#x27;, &#xd;&#xa;		Backorders.PRICE &#x27;Backorder Revenue&#x27;,&#xd;&#xa;		0 as Modified_NCM,&#xd;&#xa;		0 as Net_Revenue,&#xd;&#xa;		0 as RPR,&#xd;&#xa;		0 as Gross_Revenue,&#xd;&#xa;		0 as Modified_NCM_Forecast,&#xd;&#xa;		0 as Rev_Forecast,&#xd;&#xa;		0 as Return_Rev_Forecast&#xd;&#xa;	FROM &#xd;&#xa;	&#x28;&#xd;&#xa;		&#x2f;&#x2a; Main CFO Query&#x2a;&#x2f; &#xd;&#xa;		SELECT &#xd;&#xa;			topCategory, &#xd;&#xa;			subCategory, &#xd;&#xa;			EcomCat, &#xd;&#xa;			Team, &#xd;&#xa;			sum&#x28;P1_GM_DLLS&#x29; AS P1_GM_DLLS, &#xd;&#xa;			sum&#x28;P1_Revenue&#x29; &#x2b; sum&#x28;P1_ShipCharged&#x29; AS P1_TRwFI, &#xd;&#xa;			sum&#x28;P2_GM_DLLS&#x29; AS P2_GM_DLLS, &#xd;&#xa;			sum&#x28;P2_Revenue&#x29; &#x2b; sum&#x28;P2_ShipCharged&#x29; AS P2_TRwFI&#xd;&#xa;		FROM&#xd;&#xa;		&#x28;&#xd;&#xa;			SELECT        &#xd;&#xa;				&#x27;Period 1&#x27; AS PERIOD, &#xd;&#xa;				CFO.topCategory, &#xd;&#xa;				CFO.subCategory, &#xd;&#xa;				CFO.EcomCat, &#xd;&#xa;				CFO.shipDateMonth, &#xd;&#xa;				CFO.shipDateYear, &#xd;&#xa;				CAT.SMOOTHIE_BUYER Team, &#xd;&#xa;				CFO.SHIP_DT, &#xd;&#xa;				CFO.Product_Revenue AS P1_Revenue, &#xd;&#xa;				CFO.ShipCharged AS P1_ShipCharged, &#xd;&#xa;				CFO.Gross_Margin_DLLS AS P1_GM_DLLS, &#xd;&#xa;				0 AS P2_Revenue, &#xd;&#xa;				0 AS P2_ShipCharged, &#xd;&#xa;				0 AS P2_GM_DLLS&#xd;&#xa;			FROM Ecomedate.dbo.CFO_REPORT2 CFO &#xd;&#xa;			LEFT JOIN Ecomedate.dbo.CATEGORIES CAT &#xd;&#xa;			  ON CFO.EcomCat &#x3d; CAT.PRODUCT_CATEGORY&#xd;&#xa;			WHERE ORD_ITM_STA_LGCY_CD NOT IN &#x28;&#x27;C&#x27;, &#x27;F&#x27;, &#x27;N&#x27;&#x29; &#xd;&#xa;			  AND ORD_SUB_STA_LGCY_CD IN &#x28;&#x27;S&#x27;, &#x27;X&#x27;&#x29; &#xd;&#xa;			  AND CFO.SHIP_DT BETWEEN &#xd;&#xa;					case when &#x28;year&#x28;getdate&#x28;&#x29;&#x29; &#x25; 400 &#x3d; 0 or &#x28;year&#x28;getdate&#x28;&#x29;&#x29; &#x25; 4 &#x3d; 0 and year&#x28;getdate&#x28;&#x29;&#x29; &#x25; 100 &#x3c;&#x3e; 0&#x29;&#x29; then DATEADD&#x28;MONTH, DATEDIFF&#x28;MONTH, &#x27;19010101&#x27;, GETDATE&#x28;&#x29; - 1&#x29;, &#x27;19000101&#x27;&#x29; &#x2b; 2 else DATEADD&#x28;MONTH, DATEDIFF&#x28;MONTH, &#x27;19010101&#x27;, GETDATE&#x28;&#x29; - 1&#x29;, &#x27;19000101&#x27;&#x29; &#x2b; 1 end AND &#xd;&#xa;					case when &#x28;year&#x28;getdate&#x28;&#x29;&#x29; &#x25; 400 &#x3d; 0 or &#x28;year&#x28;getdate&#x28;&#x29;&#x29; &#x25; 4 &#x3d; 0 and year&#x28;getdate&#x28;&#x29;&#x29; &#x25; 100 &#x3c;&#x3e; 0&#x29;&#x29; then DATEADD&#x28;day, DATEDIFF&#x28;day, &#x27;19010101&#x27;, GETDATE&#x28;&#x29;&#x29;, &#x27;19000101&#x27;&#x29; else DATEADD&#x28;day, DATEDIFF&#x28;day, &#x27;19010101&#x27;, GETDATE&#x28;&#x29;&#x29;, &#x27;19000101&#x27;&#x29; - 1 end&#xd;&#xa;			  &#xd;&#xa;			UNION ALL&#xd;&#xa;        &#xd;&#xa;			SELECT        &#xd;&#xa;				&#x27;Period 2&#x27; AS PERIOD, &#xd;&#xa;				CFO.topCategory, &#xd;&#xa;				CFO.subCategory, &#xd;&#xa;				CFO.EcomCat, &#xd;&#xa;				CFO.shipDateMonth, &#xd;&#xa;				CFO.shipDateYear, &#xd;&#xa;				CAT.SMOOTHIE_BUYER Team, &#xd;&#xa;				CFO.SHIP_DT, &#xd;&#xa;				0 AS P1_Revenue, &#xd;&#xa;				0 AS P1_ShipCharged, &#xd;&#xa;				0 AS P1_GM_DLLS, &#xd;&#xa;				CFO.Product_Revenue AS P2_Revenue, &#xd;&#xa;				CFO.ShipCharged AS P2_ShipCharged, &#xd;&#xa;				CFO.Gross_Margin_DLLS AS P2_GM_DLLS&#xd;&#xa;			FROM Ecomedate.dbo.CFO_REPORT2 CFO &#xd;&#xa;			LEFT JOIN Ecomedate.dbo.CATEGORIES CAT &#xd;&#xa;			  ON CFO.EcomCat &#x3d; CAT.PRODUCT_CATEGORY&#xd;&#xa;			WHERE ORD_ITM_STA_LGCY_CD NOT IN &#x28;&#x27;C&#x27;, &#x27;F&#x27;, &#x27;N&#x27;&#x29; &#xd;&#xa;			  AND ORD_SUB_STA_LGCY_CD IN &#x28;&#x27;S&#x27;, &#x27;X&#x27;&#x29; &#xd;&#xa;			  AND SHIP_DT BETWEEN CONVERT&#x28;varchar&#x28;10&#x29;, dateadd&#x28;d, - &#x28;day&#x28;getdate&#x28;&#x29; - 1&#x29;&#x29;, getdate&#x28;&#x29;&#x29;, 120&#x29; AND CONVERT&#x28;varchar&#x28;10&#x29;, GetDate&#x28;&#x29; - 1, 120&#x29;&#xd;&#xa;	&#x29; A&#xd;&#xa;	GROUP BY topCategory, subCategory, EcomCat, Team&#x29; CFO_Data &#xd;&#xa;	FULL OUTER JOIN&#xd;&#xa;	&#x28;&#xd;&#xa;		&#x2f;&#x2a; Forecast Query&#x2a;&#x2f; &#xd;&#xa;		SELECT DISTINCT &#xd;&#xa;			MCLC.LONG_NM topCategory, &#xd;&#xa;			CAT.CATEGORY subCategory, &#xd;&#xa;			F.Forecast_Name EcomCat, &#xd;&#xa;			&#x28;F.Revenue &#x2f; DT.TotalShippingDaysinMonth&#x29; &#x2a; DT.MaxShippingDay AS Revenue_calc, &#xd;&#xa;			&#x28;F.Margin &#x2f; DT.TotalShippingDaysinMonth&#x29; &#x2a; DT.MaxShippingDay AS Margin_calc&#xd;&#xa;		FROM Ecomedate.dbo.Forecast F &#xd;&#xa;		INNER JOIN &#xd;&#xa;		&#x28;&#xd;&#xa;			SELECT        &#xd;&#xa;				RPT.FULL_DATE, &#xd;&#xa;				RPT.YEAR_, &#xd;&#xa;				RPT.MONTH_NUMBER, &#xd;&#xa;				SDC.ShippingDayofMonth, &#xd;&#xa;				SDC.TotalShippingDaysinMonth, &#xd;&#xa;				SDC.IsHoliday, &#xd;&#xa;				RPT.WEEKEND_FLAG, &#xd;&#xa;				max&#x28;SDC.ShippingDayofMonth&#x29; OVER &#x28;partition BY RPT.YEAR_, RPT.MONTH_NUMBER&#x29; MaxShippingDay&#xd;&#xa;			FROM Ecomedate.dbo.ShipDayCalendar SDC &#xd;&#xa;			LEFT JOIN Ecomedate.dbo.REF_PERIOD_TABLE RPT &#xd;&#xa;			  ON SDC.DT &#x3d; RPT.FULL_DATE&#xd;&#xa;			WHERE DT BETWEEN DATEADD&#x28;MONTH, DATEDIFF&#x28;MONTH, 1, GETDATE&#x28;&#x29; - 1&#x29;, &#x27;19000101&#x27;&#x29; AND CONVERT&#x28;varchar&#x28;10&#x29;, DateAdd&#x28;yy, 0, GetDate&#x28;&#x29; - 1&#x29;, 120&#x29;&#xd;&#xa;		&#x29; DT &#xd;&#xa;			  ON F.Year &#x3d; DT.YEAR_ AND F.Period &#x3d; DT.MONTH_NUMBER &#xd;&#xa;			LEFT JOIN Ecomedate.dbo.CATEGORIES CAT &#xd;&#xa;			  ON F.Forecast_Name &#x3d; CAT.PRODUCT_CATEGORY &#xd;&#xa;			LEFT JOIN Ecomedate.dbo.REF_MAJ_CAT_LGCY_CD MCLC &#xd;&#xa;			  ON CAT.MAJOR_CATEGORY_CODE &#x3d; MCLC.MAJ_CAT_LGCY_CD&#xd;&#xa;			WHERE CAT.CATEGORY IS NOT NULL&#xd;&#xa;	&#x29; Forecast &#xd;&#xa;	  ON CFO_Data.EcomCat &#x3d; Forecast.EcomCat &#xd;&#xa;	FULL OUTER JOIN&#xd;&#xa;	&#x28;&#xd;&#xa;		&#x2f;&#x2a;BackOrders Query&#x2a;&#x2f; &#xd;&#xa;		SELECT DISTINCT &#xd;&#xa;			ITM.CATEGORY EcomCat, &#xd;&#xa;			CAT.SMOOTHIE_BUYER Team, &#xd;&#xa;			sum&#x28;CASE WHEN LEFT&#x28;ITM.ITM_CD, 2&#x29; IN &#x28;&#x27;ES&#x27;, &#x27;KF&#x27;&#x29; THEN SOL.EXT_PRC_AMT &#x2a; .3 ELSE SOL.EXT_PRC_AMT &#x2a; .2 END&#x29; AS EST_MARGIN, &#xd;&#xa;			sum&#x28;SOL.EXT_PRC_AMT&#x29; PRICE&#xd;&#xa;		FROM Ecomedate.dbo.SV_MACORD_BACK_ORDER BO &#xd;&#xa;		INNER JOIN &#xd;&#xa;		&#x28;&#xd;&#xa;			SELECT&#xd;&#xa;				EDP_NO_ID, &#xd;&#xa;				sum&#x28;TOT_TO_BE_REC_QTY&#x29; QTY, &#xd;&#xa;				CONVERT&#x28;varchar&#x28;10&#x29;, min&#x28;NEW_REQ_ITM_DT&#x29;, 120&#x29; DUE_DT&#xd;&#xa;			FROM Ecomedate.dbo.SV_MACITM_PO_DETAILS&#xd;&#xa;			WHERE TOT_TO_BE_REC_QTY &#x3e; 0 &#xd;&#xa;			  AND PURCH_ORD_ITM_STA_LGCY_CD &#x3d; &#x27;OP&#x27; &#xd;&#xa;			  AND NEW_REQ_ITM_DT &#x3e;&#x3d; CONVERT&#x28;varchar&#x28;10&#x29;, GETDATE&#x28;&#x29;, 120&#x29; &#xd;&#xa;			  AND NEW_REQ_ITM_DT &#x3c; DATEADD&#x28;MONTH, DATEDIFF&#x28;MONTH, 0, GETDATE&#x28;&#x29;&#x29; &#x2b; 1, 0&#x29;&#xd;&#xa;			GROUP BY EDP_NO_ID&#xd;&#xa;		&#x29; Open_PO &#xd;&#xa;		  ON BO.EDP_NO_ID &#x3d; Open_PO.EDP_NO_ID &#xd;&#xa;		INNER JOIN Ecomedate.dbo.SV_IT_1000_ITEM_MAST ITM &#xd;&#xa;		  ON BO.EDP_NO_ID &#x3d; ITM.EDP_NO_ID &#xd;&#xa;		INNER JOIN Ecomedate.dbo.SV_OR_1000_ORDER_HEADER OH &#xd;&#xa;		  ON BO.SALES_ORD_NR &#x3d; OH.SALES_ORD_NR &#xd;&#xa;		INNER JOIN Ecomedate.dbo.SV_MACORD_SALES_ORD_LINES SOL &#xd;&#xa;		  ON BO.SALES_ORD_NR &#x3d; SOL.SALES_ORD_NR &#xd;&#xa;		  AND BO.SALES_ORD_LN_NR &#x3d; SOL.SALES_ORD_LN_NR &#xd;&#xa;		  AND OH.SALES_ORD_NR &#x3d; SOL.SALES_ORD_NR &#xd;&#xa;		LEFT JOIN Ecomedate.dbo.CATEGORIES CAT &#xd;&#xa;		  ON ITM.CATEGORY &#x3d; CAT.PRODUCT_CATEGORY&#xd;&#xa;		WHERE ITM.CATEGORY NOT IN &#x28;&#x27;OOO&#x27;, &#x27;SND&#x27;, &#x27;ERP&#x27;&#x29;&#xd;&#xa;		GROUP BY ITM.CATEGORY, CAT.SMOOTHIE_BUYER&#xd;&#xa;	&#x29; Backorders &#xd;&#xa;	  ON CFO_Data.EcomCat &#x3d; Backorders.EcomCat &#xd;&#xa;	&#xd;&#xa;	union all&#xd;&#xa;	&#xd;&#xa;--	&#x2f;&#x2a; GMROI Query 1 - Prev Month&#x2a;&#x2f; &#xd;&#xa;		SELECT &#xd;&#xa;			GM.Category topCategory, &#xd;&#xa;			GM.SubCategory subCategory, &#xd;&#xa;			CAT.SMOOTHIE_BUYER Team, &#xd;&#xa;--			GM.EcomCat, &#xd;&#xa;			0 as &#x27;PY Gross Margin Dollars&#x27;,&#xd;&#xa;			0 as &#x27;PY Total Revenue with Freight In&#x27;,&#xd;&#xa;			0 as &#x27;CY Gross Margin Dollars&#x27;,&#xd;&#xa;			0 as &#x27;CY Total Revenue with Freight In&#x27;,&#xd;&#xa;			0 as &#x27;Forecast Margin&#x27;, &#xd;&#xa;			0 as &#x27;Forecast Revenue&#x27;, &#xd;&#xa;			sum&#x28;Total_Gross_Margin&#x29; &#x27;Prev_Month_TGM&#x27;, &#xd;&#xa;			sum&#x28;Marketing_Spend&#x29; &#x27;Prev_Month_MS&#x27;, &#xd;&#xa;--			CASE WHEN sum&#x28;Marketing_Spend&#x29; &#x3d; 0 THEN 0 ELSE sum&#x28;Total_Gross_Margin&#x29; &#x2f; sum&#x28;Marketing_Spend&#x29; END AS Prev_Month_GMROI&#xd;&#xa;			0 as &#x27;Curr_Month_TGM&#x27;,&#xd;&#xa;			0 as &#x27;Curr_Month_MS&#x27;,&#xd;&#xa;			0 as &#x27;Backorder Margin&#x27;,&#xd;&#xa;			0 as &#x27;Backorder Revenue&#x27;,&#xd;&#xa;			0 as Modified_NCM,&#xd;&#xa;			0 as Net_Revenue,&#xd;&#xa;			0 as RPR,&#xd;&#xa;			0 as Gross_Revenue,&#xd;&#xa;			0 as Modified_NCM_Forecast,&#xd;&#xa;			0 as Rev_Forecast,&#xd;&#xa;			0 as Return_Rev_Forecast&#xd;&#xa;		FROM Ecomedate.dbo.ext_GMROI_by_Category_and_SalesChannel GM &#xd;&#xa;		LEFT JOIN Ecomedate.dbo.CATEGORIES CAT &#xd;&#xa;		  ON GM.EcomCat &#x3d; CAT.PRODUCT_CATEGORY&#xd;&#xa;		WHERE Order_Entry_Date &#x3e;&#x3d; DATEADD&#x28;MONTH, DATEDIFF&#x28;MONTH, 0, GETDATE&#x28;&#x29;&#x29; - 1, 0&#x29; &#xd;&#xa;		  AND Order_Entry_Date &#x3c; DATEADD&#x28;MONTH, DATEDIFF&#x28;MONTH, 0, GETDATE&#x28;&#x29;&#x29; - 0, 0&#x29;&#xd;&#xa;		GROUP BY &#xd;&#xa;			GM.Category, &#xd;&#xa;			GM.SubCategory, &#xd;&#xa;--			GM.EcomCat, &#xd;&#xa;			CAT.SMOOTHIE_BUYER&#xd;&#xa;	&#xd;&#xa;	union all&#xd;&#xa;&#xd;&#xa;--		&#x2f;&#x2a; GMROI Query 2 - Curr Month&#x2a;&#x2f; &#xd;&#xa;		SELECT &#xd;&#xa;			GM.Category topCategory, &#xd;&#xa;			GM.SubCategory subCategory, &#xd;&#xa;--			GM.EcomCat, &#xd;&#xa;			CAT.SMOOTHIE_BUYER Team, &#xd;&#xa;			0 as &#x27;PY Gross Margin Dollars&#x27;,&#xd;&#xa;			0 as &#x27;PY Total Revenue with Freight In&#x27;,&#xd;&#xa;			0 as &#x27;CY Gross Margin Dollars&#x27;,&#xd;&#xa;			0 as &#x27;CY Total Revenue with Freight In&#x27;,&#xd;&#xa;			0 as &#x27;Forecast Margin&#x27;, &#xd;&#xa;			0 as &#x27;Forecast Revenue&#x27;, &#xd;&#xa;			0 as Prev_Month_TGM, &#xd;&#xa;			0 as Prev_Month_MS,&#xd;&#xa;			sum&#x28;Total_Gross_Margin&#x29; Curr_Month_TGM, &#xd;&#xa;			sum&#x28;Marketing_Spend&#x29; Curr_Month_MS, &#xd;&#xa;--			CASE WHEN sum&#x28;Marketing_Spend&#x29; &#x3d; 0 THEN 0 ELSE sum&#x28;Total_Gross_Margin&#x29; &#x2f; sum&#x28;Marketing_Spend&#x29; END AS Curr_Month_GMROI&#xd;&#xa;			0 as &#x27;Backorder Margin&#x27;,&#xd;&#xa;			0 as &#x27;Backorder Revenue&#x27;,&#xd;&#xa;			0 as Modified_NCM,&#xd;&#xa;			0 as Net_Revenue,&#xd;&#xa;			0 as RPR,&#xd;&#xa;			0 as Gross_Revenue,&#xd;&#xa;			0 as Modified_NCM_Forecast,&#xd;&#xa;			0 as Rev_Forecast,&#xd;&#xa;			0 as Return_Rev_Forecast&#xd;&#xa;		FROM Ecomedate.dbo.ext_GMROI_by_Category_and_SalesChannel GM &#xd;&#xa;		LEFT JOIN Ecomedate.dbo.CATEGORIES CAT &#xd;&#xa;		  ON GM.EcomCat &#x3d; CAT.PRODUCT_CATEGORY&#xd;&#xa;		WHERE Order_Entry_Date &#x3e;&#x3d; DATEADD&#x28;MONTH, DATEDIFF&#x28;MONTH, 0, GETDATE&#x28;&#x29;&#x29; - 0, 0&#x29;&#xd;&#xa;		GROUP BY &#xd;&#xa;			GM.Category, &#xd;&#xa;			GM.SubCategory, &#xd;&#xa;--			GM.EcomCat, &#xd;&#xa;			CAT.SMOOTHIE_BUYER&#xd;&#xa;&#xd;&#xa;	union all&#xd;&#xa;&#xd;&#xa;	select &#xd;&#xa;		CMR.MajorCategory,&#xd;&#xa;		isnull&#x28;Forecast_MCM.subCategory, &#x27;_MTD_MCM_Goal&#x27;&#x29; as subCategory,&#xd;&#xa;		null as Team,&#xd;&#xa;		0 as &#x27;PY Gross Margin Dollars&#x27;,&#xd;&#xa;		0 as &#x27;PY Total Revenue with Freight In&#x27;,&#xd;&#xa;		0 as &#x27;CY Gross Margin Dollars&#x27;,&#xd;&#xa;		0 as &#x27;CY Total Revenue with Freight In&#x27;,&#xd;&#xa;		0 as &#x27;Forecast Margin&#x27;, &#xd;&#xa;		0 as &#x27;Forecast Revenue&#x27;, &#xd;&#xa;		0 as &#x27;Prev_Month_TGM&#x27;,&#xd;&#xa;		0 as &#x27;Prev_Month_MS&#x27;,&#xd;&#xa;		0 as &#x27;Curr_Month_TGM&#x27;,&#xd;&#xa;		0 as &#x27;Curr_Month_MS&#x27;,&#xd;&#xa;		0 as &#x27;Backorder Margin&#x27;,&#xd;&#xa;		0 as &#x27;Backorder Revenue&#x27;,&#xd;&#xa;		sum&#x28;Product_Revenue &#x2b; Shipping_Charged - Direct_Product_Costs - Freight_Out - Rebate_Discount&#x29; &#x2b; sum&#x28;Return_Product_Revenue&#x29; &#xd;&#xa;			&#x2b; sum&#x28;Return_Product_Costs&#x29; - sum&#x28;Inventory_Freight_Returns&#x29; - sum&#x28;Inventory_Adjustment&#x29; - sum&#x28;Other_Credit&#x29; &#xd;&#xa;			- sum&#x28;fixedCosts &#x2b; PPC_Costs &#x2b; CSE_Costs &#x2b; Affiliate_Costs &#x2b; Facebook_Costs &#x2b; Pinterest_Costs&#x29; - sum&#x28;TotalMerchantFees&#x29; as Modified_NCM,&#xd;&#xa;		sum&#x28;Total_Revenue_with_Freight_In&#x29; &#x2b; sum&#x28;Return_Product_Revenue&#x29; as Net_Revenue,&#xd;&#xa;		-1 &#x2a; sum&#x28;Return_Product_Revenue&#x29; RPR,&#xd;&#xa;		sum&#x28;Total_Revenue_with_Freight_In&#x29; Gross_Revenue,&#xd;&#xa;&#xd;&#xa;		sum&#x28;Forecast_MCM.MCM_Forecast&#x29; &#x2f; count&#x28;CMR.MajorCategory&#x29; Modified_NCM_Forecast,&#xd;&#xa;		sum&#x28;Forecast_Revenue.Rev_Forecast&#x29; &#x2f;  count&#x28;CMR.MajorCategory&#x29; Rev_Forecast,&#xd;&#xa;		&#x28;sum&#x28;Forecast_Return_Rev.Return_Forecast&#x29; &#x2f; count&#x28;CMR.MajorCategory&#x29;&#x29; Return_Rev_Forecast&#xd;&#xa;&#xd;&#xa;	from Ecomedate.dbo.ext_CM_with_Returns_and_Marketing_Spend CMR&#xd;&#xa;	left join &#xd;&#xa;	&#x28;&#xd;&#xa;		select &#xd;&#xa;			F.MajorCategory,&#xd;&#xa;			F.subCategory,&#xd;&#xa;			F.Value,&#xd;&#xa;			F.Value_Percent,&#xd;&#xa;			&#x28; F.Value &#x2f; F.Ship_Days_in_Period &#x29; &#x2a; SDC.ShippingDayofMonth as MCM_Forecast&#xd;&#xa;		from Ecomedate.dbo.ShipDayCalendar SDC&#xd;&#xa;		left join Ecomedate.&#x5b;dbo&#x5d;.&#x5b;ext_Modified_CM_Forecast&#x5d; F&#xd;&#xa;		  on SDC.Year &#x3d; F.Year&#xd;&#xa;		  and SDC.Period &#x3d; F.Period&#xd;&#xa;		where SDC.DT &#x3d; convert&#x28;varchar&#x28;10&#x29;, getdate&#x28;&#x29;-1, 120&#x29;&#xd;&#xa;		  and Forecast_Type &#x3d; &#x27;MCM_Forecast&#x27;&#xd;&#xa;	&#x29; Forecast_MCM&#xd;&#xa;	  on CMR.MajorCategory &#x3d; Forecast_MCM.MajorCategory&#xd;&#xa;	left join&#xd;&#xa;	&#x28;&#xd;&#xa;		select &#xd;&#xa;			F.MajorCategory,&#xd;&#xa;			F.subCategory,&#xd;&#xa;			F.Value,&#xd;&#xa;			F.Value_Percent,&#xd;&#xa;			&#x28; F.Value &#x2f; F.Ship_Days_in_Period &#x29; &#x2a; SDC.ShippingDayofMonth as Return_Forecast&#xd;&#xa;		from Ecomedate.dbo.ShipDayCalendar SDC&#xd;&#xa;		left join Ecomedate.&#x5b;dbo&#x5d;.&#x5b;ext_Modified_CM_Forecast&#x5d; F&#xd;&#xa;		  on SDC.Year &#x3d; F.Year&#xd;&#xa;		  and SDC.Period &#x3d; F.Period&#xd;&#xa;		where SDC.DT &#x3d; convert&#x28;varchar&#x28;10&#x29;, getdate&#x28;&#x29;-1, 120&#x29;&#xd;&#xa;		  and Forecast_Type &#x3d; &#x27;Return_Forecast&#x27;&#xd;&#xa;	&#x29; Forecast_Return_Rev&#xd;&#xa;	  on CMR.MajorCategory &#x3d; Forecast_Return_Rev.MajorCategory&#xd;&#xa;	left join &#xd;&#xa;	&#x28;&#xd;&#xa;		select &#xd;&#xa;			MCLC.SHORT_NM MajorCategory,&#xd;&#xa;			sum&#x28;&#x28;F.Revenue &#x2f; F.Ship_Days_in_Period &#x29; &#x2a; SDC.ShippingDayofMonth&#x29; as Rev_Forecast&#xd;&#xa;		from Ecomedate.dbo.ShipDayCalendar SDC&#xd;&#xa;		left join Ecomedate.dbo.Forecast F&#xd;&#xa;		  on SDC.Year &#x3d; F.Year&#xd;&#xa;		  and SDC.Period &#x3d; F.Period&#xd;&#xa;		left join Ecomedate.dbo.CATEGORIES C&#xd;&#xa;		  on F.Forecast_Name &#x3d; C.PRODUCT_CATEGORY&#xd;&#xa;		left join Ecomedate.dbo.REF_MAJ_CAT_LGCY_CD MCLC&#xd;&#xa;		  on C.MAJOR_CATEGORY_CODE &#x3d; MCLC.MAJ_CAT_LGCY_CD&#xd;&#xa;		where SDC.DT &#x3d; convert&#x28;varchar&#x28;10&#x29;, getdate&#x28;&#x29;-1, 120&#x29;&#xd;&#xa;		group by&#xd;&#xa;			MCLC.SHORT_NM&#xd;&#xa;	&#x29; Forecast_Revenue&#xd;&#xa;	  on CMR.MajorCategory &#x3d; case when Forecast_Revenue.MajorCategory &#x3d; &#x27;Beverage Fridge&#x27; then &#x27;Beverage Refrigerators&#x27; else Forecast_Revenue.MajorCategory end&#xd;&#xa;	where Date between DATEADD&#x28;MONTH, DATEDIFF&#x28;MONTH, 0, DATEADD&#x28;DAY,-1,GETDATE&#x28;&#x29;&#x29;&#x29;, 0&#x29; and convert&#x28;varchar&#x28;10&#x29;, getdate&#x28;&#x29;-1, 120&#x29;&#xd;&#xa;	group by&#xd;&#xa;		CMR.MajorCategory,&#xd;&#xa;		isnull&#x28;Forecast_MCM.subCategory, &#x27;_MTD_MCM_Goal&#x27;&#x29;&#xd;&#xa;	&#xd;&#xa;&#x29; A&#xd;&#xa;--where &#x5b;Major Category&#x5d; is not null &#xd;&#xa;-- and &#x5b;Major Category&#x5d; not in &#x28;&#x27;Advertising&#x27;, &#x27;Unassigned&#x27;&#x29;&#xd;&#xa;-- and SubCategory &#x3c;&#x3e; &#x27;_MTD_MCM_Goal&#x27;&#xd;&#xa;group by&#xd;&#xa;	&#x5b;Major Category&#x5d;,&#xd;&#xa;	SubCategory,&#xd;&#xa;	Team&#xd;&#xa;order by &#xd;&#xa;	&#x5b;Major Category&#x5d;,&#xd;&#xa;	SubCategory,&#xd;&#xa;	Team</sql>
