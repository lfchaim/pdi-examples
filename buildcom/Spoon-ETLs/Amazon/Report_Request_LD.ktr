import javax.crypto.Mac;
import javax.crypto.spec.SecretKeySpec;
import org.apache.commons.codec.binary.Base64;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) 
	{
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }
 
 	String NewURL = get(Fields.In, "NewURL2").getString(r);
	String SecretKey = get(Fields.In, "SecretKey").getString(r);

	
	//String EncodedURL = urlEncode(NewURL);	
	String EncodedURL = NewURL;	

	String signature = urlEncode(sign(EncodedURL, SecretKey));
	
	Object[] out_row = createOutputRow(r, data.outputRowMeta.size());
    get(Fields.Out, "EncodedURL").setValue(out_row,EncodedURL);
    get(Fields.Out, "signature").setValue(out_row,signature);
    putRow(data.outputRowMeta, out_row);

	return true;
}

private static String urlEncode(String rawValue) 
{
  String value = (rawValue == null) ? "" : rawValue;
  String encoded = null;
try 
{
  encoded = URLEncoder.encode(value, "UTF-8")
  .replace("+", "%20")
  .replace("*", "%2A")
  .replace("%7E","~");
 } 
catch (UnsupportedEncodingException e) 
{
  System.err.println("Unknown encoding: " + "UTF-8");
  e.printStackTrace();
}
 return encoded;
}
 
/*
 * Sign the text with the given secret key and convert to base64
 */
 private static String sign(String data, String secretKey) {
	
	String CHARACTER_ENCODING = "UTF-8";
	String ALGORITHM = "HmacSHA256";
	String signatureBase64 = "";
 	
try {
	Mac mac = Mac.getInstance(ALGORITHM);
	try {
		mac.init(new SecretKeySpec(secretKey.getBytes(CHARACTER_ENCODING), ALGORITHM));
		try {
			byte[] signature = mac.doFinal(data.getBytes(CHARACTER_ENCODING));
			try {
				signatureBase64 = new String(Base64.encodeBase64(signature),CHARACTER_ENCODING);
			} catch (Exception e) {
				System.err.println("Execption 4");
		  		e.printStackTrace();
			}
		} catch (Exception e) {
			System.err.println("Exception 3");
		  	e.printStackTrace();
		}
	} catch (Exception e) {
		System.err.println("Exception 2");
	  	e.printStackTrace();
	}
} catch (NoSuchAlgorithmException e) {
  System.err.println("Unknown Algorithm: " + ALGORITHM);
  e.printStackTrace();
}  

return new String(signatureBase64);
 }
