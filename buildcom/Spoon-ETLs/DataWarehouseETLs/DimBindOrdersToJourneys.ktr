    <sql>SELECT  DimJourney.ID,FactOrder.TimeKey, FactOrder.ID as EndOrderKey,DimJourney.CustomerCode FROM FactOrder&#xd;&#xa;INNER JOIN DimJourney on DimJourney.CustomerCode &#x3d; FactOrder.CustomerCode&#xd;&#xa;LEFT OUTER join FactOrder as StartOrder on DimJourney.StartOrderKey &#x3d; StartOrder.ID&#xd;&#xa;WHERE FactOrder.dateKey &#x3c; 20151130 and DimJourney.EndOrderKey is null and StartOrder.ID is null or  &#x28;StartOrder.DateKey &#x3c; FactOrder.DateKey and StartOrder.TimeKey &#x3c; FactOrder.TimeKey&#x29;&#xd;&#xa;order by FactOrder.DateKey, FactOrder.TimeKey asc</sql>
