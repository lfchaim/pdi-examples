    <sql>SELECT salesOrderNumber,ITM_CD,USE_CD,ITM_QTY,Product_Revenue,SALES_ORD_ENTRY_DT,FNAME,LNAME,&#xd;&#xa;datediff&#x28;second,dateadd&#x28;day,DATEDIFF&#x28;day,0,SALES_ORD_ENTRY_DT&#x29;,0&#x29;,SALES_ORD_ENTRY_DT&#x29; as TimeKeyCFO,&#xd;&#xa;convert&#x28;varchar&#x28;24&#x29;,SALES_ORD_ENTRY_DT,112&#x29; as DateKeyCFO,&#xd;&#xa;substring&#x28;salesOrderNumber,1,1&#x29; as Prefix,&#xd;&#xa;1 as IsInEcom,&#xd;&#xa;ReportingName,cost,&#xd;&#xa;ISNULL&#x28;EST_SHIP_COST,0&#x29; AS EST_SHIP_COST,&#xd;&#xa;convert&#x28;varchar&#x28;24&#x29;,ShipDate,112&#x29; as ShipDate,&#xd;&#xa;convert&#x28;varchar&#x28;24&#x29;,ShipConfDate,112&#x29; as ShipConfDate,&#xd;&#xa;convert&#x28;varchar&#x28;24&#x29;,ReturnDate,112&#x29; as ReturnDate,&#xd;&#xa;Isnull&#x28;ReturnAmt,0&#x29; as ReturnAmt,&#xd;&#xa;Isnull&#x28;ReturnCost,0&#x29; as ReturnCost,&#xd;&#xa;Isnull&#x28;ReturnQTY,0&#x29; as ReturnQTY,&#xd;&#xa;CancelledFlag,&#xd;&#xa;Isnull&#x28;RET_PH,0&#x29; as RET_PH,&#xd;&#xa;CostOfGoodsSold,&#xd;&#xa;ShipmentNumber,&#xd;&#xa;ShipToNumber,&#xd;&#xa;ShipCharged,&#xd;&#xa;lineNumber,&#xd;&#xa;lineStatusCode,&#xd;&#xa;OFFER_CD&#xd;&#xa; FROM &#x28;&#xd;&#xa;SELECT header.SALES_ORD_ENTRY_DT&#xd;&#xa;      ,lines.&#x5b;SALES_ORD_NR&#x5d; as salesOrderNumber&#xd;&#xa;	  ,lines.SALES_ORD_LN_NR AS lineNumber&#xd;&#xa;      ,&#x5b;ITM_QTY&#x5d;&#xd;&#xa;      ,&#x5b;EXT_PRC_AMT&#x5d; as Product_Revenue&#xd;&#xa;      ,&#x5b;EXT_COST_AMT&#x5d; as cost&#xd;&#xa;	  ,&#x5b;ORD_ITM_STA_LGCY_CD&#x5d; as lineStatusCode&#xd;&#xa;	  ,CASE WHEN ORD_ITM_TYPE_LGCY_CD &#x3d; &#x27;C&#x27; THEN&#xd;&#xa;				0.0&#xd;&#xa;		ELSE&#xd;&#xa;				&#x5b;EXT_COST_AMT&#x5d;&#xd;&#xa;		END                                  		   AS CostOfGoodsSold&#xd;&#xa;      ,&#x5b;ORIG_PRC_AMT&#x5d;      &#xd;&#xa;	  ,lines.&#x5b;EDP_NO_ID&#x5d;&#xd;&#xa;	  ,Isnull&#x28;postage_amount&#x2f;&#x28;count&#x28;&#x2a;&#x29; over &#x28;partition by lines.SALES_ORD_NR,lines.SALES_ORD_SHIP_NR,lines.SALES_ORD_SHIP_TO_NR&#x29;&#x29;, EST_SHIP_COST &#x2a; ITM_QTY&#x29; AS EST_SHIP_COST&#xd;&#xa;	  ,Isnull&#x28;shipCharged.ShipChargedAmt&#x2f;sum&#x28;case when lines.ORD_ITM_TYPE_LGCY_CD NOT IN &#x28;&#x27;C&#x27;,&#x27;D&#x27;,&#x27;E&#x27;&#x29; then 1 else 0 end&#x29; over &#x28;partition by lines.SALES_ORD_NR,lines.SALES_ORD_SHIP_NR, lines.SALES_ORD_SHIP_TO_NR&#x29;,0&#x29;			   AS ShipCharged&#xd;&#xa;	  ,SOURCE_&#xd;&#xa;	  ,header.OFFER_CD&#xd;&#xa;	  ,header.USE_CD&#xd;&#xa;	  ,ReportingName&#xd;&#xa;	  ,FNAME&#xd;&#xa;	  ,LNAME&#xd;&#xa;	  ,rtrim&#x28;itm.ITM_CD&#x29; as ITM_CD&#xd;&#xa;	  ,RTN_PROD_AMT &#x2b; RTN_P_H_AMT &#x2b; RTN_OTHER_AMT &#x2b; RTN_TAX_AMT as ReturnAmt&#xd;&#xa;	  ,case when NO_PRODUCT_IND &#x3d; &#x27;N&#x27; then RTN_EXT_COST_AMT else 0 end as ReturnCost&#xd;&#xa;	  ,isnull&#x28;ret.QTY,0&#x29; as ReturnQTY&#xd;&#xa;	  ,RTN_DT as ReturnDate&#xd;&#xa;	  ,SHIP_DT as ShipDate&#xd;&#xa;	  ,SHIP_CONF_DT as ShipConfDate&#xd;&#xa;	  ,case when ORD_ITM_STA_LGCY_CD In &#x28;&#x27;C&#x27;, &#x27;F&#x27;, &#x27;N&#x27;&#x29; then 1 else 0 end as CancelledFlag&#xd;&#xa;	  ,RET_PH&#xd;&#xa;	  ,lines.SALES_ORD_SHIP_NR as ShipmentNumber&#xd;&#xa;	  ,lines.SALES_ORD_SHIP_TO_NR as ShipToNumber&#xd;&#xa;  FROM &#x5b;Ecomedate&#x5d;.&#x5b;dbo&#x5d;.&#x5b;SV_MACORD_SALES_ORD_LINES&#x5d; as lines&#xd;&#xa;	left join &#x28;SELECT  &#xd;&#xa;					D14AL1.SALES_ORD_NR, &#xd;&#xa;					D14AL1.SALES_ORD_SHIP_TO_NR, &#xd;&#xa;					D14AL1.SALES_ORD_SHIP_NR, &#xd;&#xa;					SUM&#x28;D14AL2.POST_AMT&#x29; AS EXPR1, &#xd;&#xa;					D14AL2.WH_CD,&#xd;&#xa;					D14AL2.CITY as SHIPTOCITY,&#xd;&#xa;					D14AL2.STATE_ as SHIPTOSTATE&#xd;&#xa;				 FROM  ecomedate.dbo.SV_MACMAN_MANI_PKG_SHIP AS D14AL1 with &#x28;NOLOCK&#x29; &#xd;&#xa;				 LEFT OUTER JOIN ecomedate.dbo.SV_MA_900_MANIFEST_ENTRIES AS D14AL2 with &#x28;NOLOCK&#x29; &#xd;&#xa;					ON D14AL1.MANI_REF_NR &#x3d; D14AL2.MANI_REF_NR &#xd;&#xa;					AND D14AL2.WH_CD IN &#x28;&#x27;&#x27;, &#x27;00&#x27;&#x29; &#xd;&#xa;					AND D14AL1.MANI_PKG_SEQ_NR &#x3d; D14AL2.MANI_PKG_SEQ_NR &#xd;&#xa;				 GROUP BY &#xd;&#xa;					D14AL1.SALES_ORD_NR, &#xd;&#xa;					D14AL1.SALES_ORD_SHIP_TO_NR, &#xd;&#xa;					D14AL1.SALES_ORD_SHIP_NR, &#xd;&#xa;					D14AL2.WH_CD,&#xd;&#xa;					D14AL2.CITY,&#xd;&#xa;					D14AL2.STATE_&#x29; AS AL15 &#x28;order_number, ship_to_number, shipment_number, postage_amount, warehouse_code, city, state&#x29; on AL15.order_number &#x3d; lines.SALES_ORD_NR and AL15.ship_to_number &#x3d; lines.SALES_ORD_SHIP_TO_NR and AL15.shipment_number &#x3d; lines.SALES_ORD_SHIP_NR&#xd;&#xa;  left join EST_SHIPPING on lines.EDP_NO_ID &#x3d; EDP_NUMBER&#xd;&#xa;  left join SV_IT_1000_ITEM_MAST as itm on lines.EDP_NO_ID &#x3d; itm.EDP_NO&#xd;&#xa;  inner join SV_MACORD_ORDER_HEADER as header on header.SALES_ORD_NR  &#x3d; lines.SALES_ORD_NR&#xd;&#xa;  left join SourceCodeReporting as SourceTable on header.USE_CD &#x3d; SourceTable.Use_CD and header.OFFER_CD &#x3d; SourceTable.Offer_CD&#xd;&#xa;  inner join SV_MACORD_CUSTOMERS as custTable on custTable.CUST_EDP_ID&#x3d; header.CUST_EDP&#xd;&#xa;  left 	join SV_MACORD_RETURNS as ret on lines.SALES_ORD_NR &#x3d; ret.SALES_ORD_NR and lines.SALES_ORD_SHIP_NR &#x3d; ret.SALES_ORD_SHIP_NR and lines.SALES_ORD_SHIP_TO_NR &#x3d; ret.SALES_ORD_SHIP_TO_NR and lines.EDP_NO_ID &#x3d; ret.EDP_NO_ID&#xd;&#xa;               LEFT JOIN &#x28;SELECT ship.SALES_ORD_NR,&#xd;&#xa;                                 ship.SALES_ORD_SHIP_TO_NR,&#xd;&#xa;                                 SALES_ORD_SHIP_NR,&#xd;&#xa;                                 FIN_TRANS_AMT AS ShipChargedAmt&#xd;&#xa;                          FROM   SV_MACORD_FT_SHIP_AMT AS ship&#xd;&#xa;                          WHERE  ship.FIN_TRANS_LGCY_CD IN &#x28;&#xd;&#xa;                                 &#x27;11&#x27;, &#x27;12&#x27;, &#x27;13&#x27;, &#x27;14&#x27;,&#xd;&#xa;                                 &#x27;15&#x27;, &#x27;16&#x27;, &#x27;17&#x27;, &#x27;18&#x27; &#x29;&#xd;&#xa;                                 AND ship.FIN_AMT_TYPE_LGCY_CD &#x3d; &#x27;PHCG&#x27;&#xd;&#xa;                          GROUP  BY ship.SALES_ORD_NR,&#xd;&#xa;                                    ship.SALES_ORD_SHIP_TO_NR,&#xd;&#xa;                                    ship.SALES_ORD_SHIP_NR,&#xd;&#xa;                                    FIN_TRANS_AMT&#x29; AS shipCharged&#xd;&#xa;                      ON shipCharged.SALES_ORD_NR &#x3d; lines.SALES_ORD_NR&#xd;&#xa;                         AND shipCharged.SALES_ORD_SHIP_TO_NR &#x3d;&#xd;&#xa;                             lines.SALES_ORD_SHIP_TO_NR&#xd;&#xa;                         AND shipCharged.SALES_ORD_SHIP_NR &#x3d;&#xd;&#xa;                             lines.SALES_ORD_SHIP_NR&#xd;&#xa;                         AND lines.ORD_ITM_TYPE_LGCY_CD IN &#x28; &#x27;H&#x27;, &#x27;R&#x27; &#x29;&#xd;&#xa;                         AND lines.ORD_ITM_TYPE_LGCY_CD NOT IN &#x28; &#x27;C&#x27;, &#x27;D&#x27;, &#x27;E&#x27; &#x29;  &#xd;&#xa;	where RTN_DT &#x3e; dateadd&#x28;day,-10, &#x27;&#x24;&#x7b;startDate&#x7d;&#x27;&#x29; or &#xd;&#xa;		header.SRC_SYS_TRANS_DT &#x3e; dateadd&#x28;day,0,&#x27;&#x24;&#x7b;startDate&#x7d;&#x27;&#x29; and header.SALES_ORD_ENTRY_DT &#x3c; &#x27;&#x24;&#x7b;endDate&#x7d;&#x27;&#x29; as inside&#xd;&#xa;--where &#x28;CancelledFlag &#x3d; 1 or ReturnQTY &#x3e; 0 or SALES_ORD_ENTRY_DT &#x3e; dateadd&#x28;day,-5,&#x27;&#x24;&#x7b;startDate&#x7d;&#x27;&#x29;&#x29;&#xd;&#xa;--WHERE salesOrderNumber &#x3d; &#x27;W0291003&#x27;&#xd;&#xa;--order by salesOrderNumber asc</sql>
