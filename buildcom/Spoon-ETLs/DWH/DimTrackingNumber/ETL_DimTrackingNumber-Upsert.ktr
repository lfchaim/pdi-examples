    <connection>DataWarehouse</connection>
    <sql>SELECT OrderNumber as DWHOrderNumber,&#xd;&#xa;	TrackingNumber as DWHTrackingNumber,&#xd;&#xa;	DimTrackingNumber.ID as TrackingNumberID,&#xd;&#xa;	FactOrder.ID as FactOrderID&#xd;&#xa;FROM FactOrder&#xd;&#xa;	JOIN DimDate ON FactOrder.DateKey &#x3d; DimDate.DateKey&#xd;&#xa;	LEFT JOIN DimTrackingNumber ON TrackingNumberKey &#x3d; DimTrackingNumber.ID&#xd;&#xa;WHERE DimDate.Date &#x3e; &#x27;20161130&#x27;&#xd;&#xa;ORDER BY OrderNumber ASC</sql>
    <limit>0</limit>
    <lookup/>
