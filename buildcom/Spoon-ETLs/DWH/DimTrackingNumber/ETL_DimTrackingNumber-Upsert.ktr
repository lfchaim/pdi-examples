  <step>
    <name>DWH Orders&#x2f;TrackingNumbers From Last 3 Months</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
