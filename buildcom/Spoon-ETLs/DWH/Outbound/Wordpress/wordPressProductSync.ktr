    <sql>SELECT&#xd;&#xa;  P.ID&#xd;&#xa;, SKU&#xd;&#xa;, CategoryKey&#xd;&#xa;, DWSKU&#xd;&#xa;, ISNULL&#x28;Description,&#x27; &#x27;&#x29; AS Description&#xd;&#xa;, B.Brand&#xd;&#xa;, BazaarVoiceRating&#xd;&#xa;, DWC.CategoryName&#xd;&#xa;, SpecificationsString&#xd;&#xa;, &#x28;ISNULL&#x28;&#x27;&#x2f;&#x27;&#x2b;PageUrl,&#x27;&#x27;&#x29;&#x2b;&#x27;&#x2f;&#x27;&#x2b;DWSKU&#x2b;&#x27;.html&#x27;&#x29; AS Link&#xd;&#xa;, &#x27; &#x27; AS Thumbnail&#xd;&#xa;, ISNULL&#x28;OC.OrderCount,0&#x29; AS OrderCount&#xd;&#xa;, EC.CategoryCode AS EcomCategory&#xd;&#xa;, MAPPrice&#xd;&#xa;, ListPrice&#xd;&#xa;, SalePrice&#xd;&#xa;, ImageUrl&#xd;&#xa;FROM DimProduct AS P&#xd;&#xa;LEFT JOIN&#xd;&#xa;DimBrand AS B&#xd;&#xa;ON B.ID &#x3d; P.BrandKey&#xd;&#xa;INNER JOIN &#xd;&#xa;BridgeDemandwareCategoriesToProducts AS BR&#xd;&#xa;ON BR.ProductKey &#x3d; P.ID&#xd;&#xa;INNER JOIN&#xd;&#xa;DimDemandwareCategory AS DWC&#xd;&#xa;ON DWC.ID &#x3d; BR.DemandwareCategoryKey&#xd;&#xa;INNER JOIN &#xd;&#xa;DimSite AS S &#xd;&#xa;ON S.ID &#x3d; BR.SiteKey&#xd;&#xa;LEFT JOIN&#xd;&#xa;DimCategory AS EC&#xd;&#xa;ON EC.ID &#x3d; P.CategoryKey&#xd;&#xa;LEFT JOIN&#xd;&#xa;&#x28;SELECT ProductKey,CAST&#x28;SUM&#x28;Revenue&#x29; AS INT&#x29; AS OrderCount FROM FactLineItem GROUP BY ProductKey&#x29; AS OC&#xd;&#xa;ON OC.ProductKey &#x3d; BR.ProductKey &#xd;&#xa;WHERE &#xd;&#xa;DWSKU IS NOT NULL&#xd;&#xa;AND &#x28;Orderable&#x3d;1 AND Searchable&#x3d;1 AND Discontinued&#x3d;0&#x29;&#xd;&#xa;AND S.Prefix &#x3d; &#x27;&#x24;&#x7b;sitePrefix&#x7d;&#x27;&#xd;&#xa;AND P.ParentKey IS NULL&#xd;&#xa;AND DWC.CategoryName &#x3c;&#x3e; &#x27;Warranties&#x27;&#xd;&#xa;AND SpecificationsString IS NOT NULL</sql>
