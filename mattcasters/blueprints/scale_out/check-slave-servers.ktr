        <jsScript_script>var transMeta &#x3d; _step_.getTrans&#x28;&#x29;.getTransMeta&#x28;&#x29;&#x3b;&#xa;&#xa;var defined &#x3d; false&#x3b;&#xa;var active &#x3d; false&#x3b;&#xa;var nrActiveTrans &#x3d; null&#x3b;&#xa;var nrActiveJobs &#x3d; null&#x3b;&#xa;var slaveStatus &#x3d; null&#x3b;&#xa;var responseTime &#x3d; null&#xa;&#xa;var currentLoad &#x3d; null&#x3b;&#xa;&#xa;  &#xa;&#x2f;&#x2f; find the slave server in the transformtaion...&#xa;&#x2f;&#x2f;&#xa;var slaveServer &#x3d; transMeta.findSlaveServer&#x28; name &#x29;&#x3b;&#xa;if &#x28;slaveServer&#x21;&#x3d;null&#x29; &#x7b;&#xa;  defined &#x3d; true&#x3b;&#xa;&#xa;  &#x2f;&#x2f; Grab the status from the slave server...&#xa;  &#x2f;&#x2f; &#xa;  try &#x7b;&#xa;    var startTime &#x3d; new Date&#x28;&#x29;&#x3b;&#xa;    slaveStatus &#x3d; slaveServer.getStatus&#x28;&#x29;&#x3b;&#xa;    var endTime &#x3d; new Date&#x28;&#x29;&#x3b;&#xa;    active &#x3d; true&#x3b;    &#xa;  &#x7d; catch&#x28;e&#x29; &#x7b;&#xa;    writeToLog&#x28;&#x22;e&#x22;, &#x22;Unable to get status of slave server &#x22;&#x2b;name&#x29;&#x3b;&#xa;  &#x7d;&#xa;&#x7d;&#xa;&#xa;&#x2f;&#x2f; Retrieve some information in case the slave server is active...&#xa;&#x2f;&#x2f;&#xa;if &#x28;active&#x29; &#x7b;&#xa;&#xa;  var startT &#x3d; new java.math.BigInteger&#x28;java.lang.Long.toString&#x28;startTime&#x29;&#x29;&#x3b;&#xa;  var endT &#x3d; new java.math.BigInteger&#x28;java.lang.Long.toString&#x28;endTime&#x29;&#x29;&#x3b;&#xa;  responseTime &#x3d; endT.subtract&#x28;startT&#x29;.toString&#x28;&#x29;&#x3b;&#xa;&#xa;  &#x2f;&#x2f; Debug&#xa;  &#x2f;&#x2f;&#xa;  &#x2f;&#x2f; Alert&#x28;&#x22;Start&#x3d;&#x22;&#x2b;startT&#x2b;&#x22;, end&#x3d;&#x22;&#x2b;endT&#x2b;&#x22; &#x3a; &#x22;&#x2b;responseTime&#x29;&#x3b;&#xa;  &#xa;  nrActiveTrans &#x3d; 0&#x3b;&#xa;  for &#x28;var trs in Iterator&#x28;slaveStatus.getTransStatusList&#x28;&#x29;&#x29;&#x29; &#x7b;&#xa;    if &#x28;trs.isRunning&#x28;&#x29;&#x29; &#x7b;&#xa;      nrActiveTrans&#x2